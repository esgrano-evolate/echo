{
    "executionRoleArn": "arn:aws:iam::{{ AWS_ACCOUNT_ID }}:role/echo-task-execution-role",
    "containerDefinitions": [
        {
            "name": "echo",
            "image": "{{ IMAGE_NAME }}",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "echo"
}
